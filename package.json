{
  "name": "grader",
  "version": "1.0.0",
  "scripts": {
    "build-css": "npm run build-css-layout && npm run build-css-home",
    "build-css-layout": "node-sass --output-style compressed --output build/css/ style/layout.scss",
    "build-css-home": "node-sass --output-style compressed --output build/css/ style/home.scss",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
